{
  "name": "arraypress/s3-providers",
  "description": "A comprehensive library facilitating interactions with various S3-compatible storage providers. Supports popular providers like Cloudflare R2, Linode, Amazon, and others.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "stable",
  "require": {
    "arraypress/s3-sanitization": "*",
    "arraypress/s3-path-resolver": "*",
    "arraypress/s3-signer": "*"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\Utils\\S3\\": "src/"
    },
    "files": ["src/Functions.php"]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/arraypress/s3-sanitization"
    },
    {
      "type": "vcs",
      "url": "https://github.com/arraypress/s3-path-resolver"
    },
    {
      "type": "vcs",
      "url": "https://github.com/arraypress/s3-signer"
    }
  ]
}